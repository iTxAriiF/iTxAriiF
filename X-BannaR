# CVALUE
blue = '\33[94m'
purple = '\033[95m'
red = '\033[91m'
white = '\33[97m'
yellow = '\33[93m'
green = '\033[1;32m'
cyan = "\033[96m"
end = '\033[0m'
print ("\033[096m")

os.system

os.system("clear")

def word (z):
  for word in z + '\n':
     sys.stdout.write(word)
     sys.stdout.flush()
     time.sleep(0.006)


word(purple+"\n\t[‚ûô] "+green+"\t [‚òÖ] "+blue+" ùôè ùôä ùôä ùôá ùôÜ ùôÑ ùôè " +green+"[‚òÖ] \t"+yellow+"[‚ûô] \n ")
